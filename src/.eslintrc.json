{
  "extends": 
  [
    "airbnb-base", 
    "prettier"
  ],
  "rules": {
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "no-use-before-define": [
      "warn",
      {
        "functions": false,
        "classes": true,
        "variables": true
      }
    ],
    "import/no-unresolved": "error",
    "import/named": "error",
    "import/no-absolute-path": "error",
    "import/no-self-import": "error",
    "import/no-useless-path-segments": "error",
    "import/no-commonjs": "error",
    "import/no-amd": "error",
    "import/export": "warn",
    "import/no-named-as-default": "warn",
    "import/dynamic-import-chunkname": "error",
    "import/no-cycle": "off",
    "prettier/prettier": "error"
  },
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "globals": {
    "gapi": true,
    "$": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "import/resolver": "node"
  }
}